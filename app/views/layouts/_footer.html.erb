<div class="grid_2">
  <div class="container">
    <div class="col-md-3 col_2">
      <h3>Tag<br>Cloud</h3>
    </div>
    <div class="col-md-9 col_5">
      <div id='tag_cloud'>
        <% tag_cloud Image.tag_counts, %w{s m l} do |tag, css_class| %>
            <%= link_to tag.name, tag_path(tag.name), class: css_class %>
        <% end %>
      </div>
    </div>
      <div class="clearfix"></div>
    </div>
    <div class="clearfix"> </div>
  </div>
</div>
